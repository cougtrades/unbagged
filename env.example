# Birdeye API Key (get free key from https://birdeye.so/)
# This is required for real market data like volume and holders
NEXT_PUBLIC_BIRDEYE_API_KEY=your_birdeye_api_key_here

# Solana RPC URL (optional - uses public RPC by default)
# For production, consider using a dedicated RPC provider
NEXT_PUBLIC_SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Note: Jupiter API doesn't require an API key for basic price data 